<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout2"
    android:layout_width="162dp"
    android:layout_height="320dp">

    <androidx.cardview.widget.CardView
        android:id="@+id/imageView9"
        android:layout_width="140dp"
        android:layout_height="263dp"
        android:backgroundTint="#FBEDE1"
        android:elevation="9dp"
        app:cardCornerRadius="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/arro"
            android:layout_width="26dp"
            android:layout_height="27dp"
            android:layout_marginStart="110dp"
            android:layout_marginTop="10dp"
            android:elevation="9dp"
            android:src="@drawable/ic_baseline_arrow_circle_right_24"
            app:layout_constraintBottom_toBottomOf="@+id/search"
            app:layout_constraintEnd_toEndOf="@+id/search"
            app:layout_constraintHorizontal_bias="0.934"
            app:layout_constraintStart_toStartOf="@+id/search"
            app:layout_constraintTop_toTopOf="@+id/search" />

        <ImageView
            android:id="@+id/imageCategoryDesgin"
            android:layout_width="86dp"
            android:layout_height="148dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/t" />

        <TextView
            android:id="@+id/nameCategorydesgin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="180dp"
            android:fontFamily="serif"
            android:text="Clothes.Wemon"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="16sp"
            android:textStyle="bold" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/update"
            android:layout_width="63dp"
            android:layout_height="23dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="220dp"

            android:background="@drawable/login_button_background"
            android:fontFamily="sans-serif-medium"
            android:text="update"
            android:textColor="#FFFFFF"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/image"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.716"
            app:layout_constraintStart_toStartOf="@+id/image"
            app:layout_constraintTop_toTopOf="@+id/image"
            app:layout_constraintVertical_bias="0.735" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/deletCateoryItem"
            android:layout_width="58dp"
            android:layout_height="23dp"
            android:layout_marginStart="75dp"
            android:layout_marginTop="220dp"

            android:background="@drawable/login_button_background"
            android:text="delete"
            android:textColor="#FFFFFF"
            android:textSize="8sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/image"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.716"
            app:layout_constraintStart_toStartOf="@+id/image"
            app:layout_constraintTop_toTopOf="@+id/image"
            app:layout_constraintVertical_bias="0.735"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck" />
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>